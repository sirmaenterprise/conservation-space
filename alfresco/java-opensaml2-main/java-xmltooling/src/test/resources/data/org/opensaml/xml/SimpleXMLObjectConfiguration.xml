<?xml version="1.0" encoding="UTF-8"?>
<xt:XMLTooling xmlns:xt="http://www.opensaml.org/xmltooling-config" 
               xmlns:test="http://www.example.org/testObjects"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >

  <xt:ObjectProviders>
    <xt:ObjectProvider qualifiedName="test:SimpleElement">
      <xt:BuilderClass className="org.opensaml.xml.mock.SimpleXMLObjectBuilder">
        <test:foo/>
      </xt:BuilderClass>
      <xt:MarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectMarshaller"/>
      <xt:UnmarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectUnmarshaller"/>
    </xt:ObjectProvider>
  </xt:ObjectProviders>
  
</xt:XMLTooling>
