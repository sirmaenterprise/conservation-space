###  http://www.sirma.com/ontologies/2013/10/culturalHeritageDomain#ConservationActivity

chd:ConservationActivity rdf:type owl:Class ;
                         
                         rdfs:label "Conservation Activity"@en ;
                         
                         rdfs:subClassOf chd:CulturalActivity ;
                         
                         emf:isSearchable "true"^^xsd:boolean ;
                         
                         skos:definition "Activities that are connected with Conservation of a cultural Objects"@en ;
                         
                         dc:creator "Kiril Penev"@en .



###  http://www.sirma.com/ontologies/2013/10/culturalHeritageDomain#ConservationExamination

chd:ConservationExamination rdf:type owl:Class ;
                            
                            rdfs:label "Conservation Examination"@en ;
                            
                            rdfs:subClassOf chd:ConservationActivity ;
                            
                            emf:isSearchable "true"^^xsd:boolean ;
                            
                            skos:definition "Conservation examination is number of activities for gathering technical and conditional information for the cultural object. Examinations can be performed for a possible treatment, a possible change of the object`s location, for possible acquisition and etc."@en ;
                            
                            dc:creator "Kiril Penev"@en .



###  http://www.sirma.com/ontologies/2013/10/culturalHeritageDomain#ConservationExaminationAction

chd:ConservationExaminationAction rdf:type owl:Class ;
                                  
                                  rdfs:label "Conservation Examination Action"@en ;
                                  
                                  rdfs:subClassOf chd:ConservationExamination ;
                                  
                                  emf:isSearchable "true"^^xsd:boolean ;
                                  
                                  skos:definition "Conservation Examination Action is a single activity performed during examination of the cultural object. Such actions are documenting the construction, the condition of the object, photographing the object and etc."@en ;
                                  
                                  dc:creator "Kiril Penev"@en .



###  http://www.sirma.com/ontologies/2013/10/culturalHeritageDomain#ConservationTreatment

chd:ConservationTreatment rdf:type owl:Class ;
                          
                          rdfs:label "Conservation Treatment"@en ;
                          
                          rdfs:subClassOf chd:ConservationActivity ;
                          
                          emf:isSearchable "true"^^xsd:boolean ;
                          
                          skos:definition "Conservation Treatment is a list of actions that has the purpouse of restoring old or damaged cultural object to their original state."@en ;
                          
                          dc:creator "Kiril Penev"@en .



###  http://www.sirma.com/ontologies/2013/10/culturalHeritageDomain#ConservationTreatmentAction

chd:ConservationTreatmentAction rdf:type owl:Class ;
                                
                                rdfs:label "Conservation Treatment Action"@en ;
                                
                                rdfs:subClassOf chd:ConservationTreatment ;
                                
                                emf:isSearchable "true"^^xsd:boolean ;
                                
                                skos:definition "Conservation Treatment Action is a single activity that is performed in order to accomplish the restoration of a cultural object. Such actions are relining a painting, attaching a new canvas to the back, surface cleaning, filling and inpainting looses etc. "@en ;
                                
                                dc:creator "Kiril Penev"@en .



